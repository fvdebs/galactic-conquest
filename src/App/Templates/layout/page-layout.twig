<!DOCTYPE html>
<html lang="{{ locale }}">
{% set version = '?555455529' %}
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <meta name="description" content="{{ 'app.page-title' | trans }}">
        <meta name="keywords" content="Browser game,galaxy,galactic,game,space">
        <title>{% block title %}{{ 'app.page-title' | trans }}{% endblock %}</title>
        <link rel="shortcut icon" type="image/x-icon" href="/assets/images/icon/favicon.ico{{ version }}">
        <link rel="manifest" href="/manifest.json{{ version }}">
        <link rel="stylesheet" href="/assets/style.css{{ version }}">
        <script src="/assets/js/matomo.js{{ version }}"></script>
        {% block style %}{% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
        <script src="/assets/js/slidebars.min.js{{ version }}"></script>
        <script src="/assets/js/gc.js{{ version }}"></script>
        <script src="/assets/js/form.min.js{{ version }}"></script>
        {% block script %}{% endblock %}
    </body>
</html>
